/var/log/syslog
{
        rotate 30
        daily
        missingok
        notifempty
        delaycompress
        dateext
        dateformat .%Y%m%d
        compress
        postrotate
                /usr/lib/rsyslog/rsyslog-rotate                
                sh /home/ubuntu/uploadtoS3/upsys
                sh /home/ubuntu/uploadtoS3/delete30
        endscript
}

/var/log/mail.info
/var/log/mail.warn
/var/log/mail.err
/var/log/mail.log
/var/log/daemon.log
/var/log/kern.log
/var/log/auth.log
/var/log/user.log
/var/log/lpr.log
/var/log/cron.log
/var/log/debug
/var/log/messages
{
        rotate 4
        weekly
        missingok
        notifempty
        compress
        delaycompress
        sharedscripts
        postrotate
                /usr/lib/rsyslog/rsyslog-rotate
        endscript
}
